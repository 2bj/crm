{
    "name": "oro/account-bundle",
    "type": "symfony-bundle",
    "description": "Oro CRM Accounts",
    "keywords": ["CRM", "Accounts"],
    "homepage": "https://github.com/orocrm/AccountBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.*",
        "friendsofsymfony/rest-bundle": "0.11.*",
        "nelmio/api-doc-bundle": "dev-master",

        "oro/ui-bundle": "dev-master",
        "oro/user-bundle": "dev-master",
        "oro/address-bundle": "dev-address_flexible_type"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "target-dir": "Oro/Bundle/AccountBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
